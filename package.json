{
  "name": "airbnb-image-optimizer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:coverage && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run lint && npm run test:coverage && npm run test:integration && npm run test:e2e",
    "lighthouse": "lhci autorun",
    "security:audit": "npm audit --audit-level moderate",
    "security:scan": "snyk test",
    "docker:build": "docker build -t airbnb-optimizer .",
    "docker:run": "docker run -p 3000:3000 airbnb-optimizer",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
