    "apify-client": "^2.8.2",
    "clsx": "^2.1.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "next": "14.2.5",
    "p-retry": "^6.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^5.8.2",
    "tailwind-merge": "^2.4.0",
